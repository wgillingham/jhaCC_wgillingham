{
  "Services": {
    "Monitor": {
      "ReconnectDelaySeconds": 15,
      "MessageMonitorTwitter": {
        "TwitterBearerToken": "",
        "FeedEndpoint": "https://api.twitter.com/2/tweets/sample/stream?tweet.fields=created_at,entities&expansions=attachments.media_keys"
      }
    },
    "Distiller": {

    },
    "Reporting": {
      "AutoReport": true,
      "AutoReportIntervalSeconds": 5,
      "AutoReportTopCount": 5,
      "ReportListenerPort": 8888
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    }
  }
}